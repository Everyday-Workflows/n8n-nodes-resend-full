{
  "name": "n8n-nodes-resend",
  "version": "2.1.0",
  "description": "Resend integration for n8n with full API coverage: Email, Broadcasts, Templates, Domains, Contacts, Segments, Topics, Webhooks, and more",
  "keywords": [
    "n8n-community-node-package",
    "resend",
    "email",
    "webhook",
    "n8n",
    "automation",
    "integration",
    "broadcast",
    "template",
    "contacts"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Everyday-Workflows/n8n-nodes-resend-full/",
  "author": {
    "name": "Everyday Workflows",
    "email": "hello@everydayworkflows.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Everyday-Workflows/n8n-nodes-resend-full.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ResendApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Resend/Resend.node.js",
      "dist/nodes/Resend/ResendTrigger.node.js"
    ]
  },
  "dependencies": {
    "isbot": "^3.6.13"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "@typescript-eslint/parser": "~8.54.0",
    "eslint": "^9.39.2",
    "eslint-plugin-n8n-nodes-base": "^1.16.5",
    "gulp": "^5.0.0",
    "jest": "^30.2.0",
    "n8n-workflow": "^2.6.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=2.6.0"
  }
}
